{
  "name": "tissue-roll",
  "version": "5.0.0",
  "description": "Very simple read/write database with a no-sql.",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/typings/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/typings/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "docs/**/*"
  ],
  "scripts": {
    "test": "jest",
    "test:unit": "jest unit.test.ts",
    "test:perf": "jest perf.test.ts --runInBand",
    "build": "node build/index.js && tsc"
  },
  "keywords": [
    "database",
    "db",
    "store"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/izure1/tissue-roll.git"
  },
  "author": "izure1 <admin@izure.org>",
  "license": "MIT",
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/xxhashjs": "^0.2.4",
    "chance": "^1.1.11",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@noble/ciphers": "^0.5.2",
    "cachebranch": "^1.2.0",
    "hookall": "^2.1.0",
    "serializable-bptree": "^4.0.0",
    "xxhashjs": "^0.2.2"
  }
}
